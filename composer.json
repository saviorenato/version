{
    "name": "saviorenato/version",
    "description": "Versionate APPs",
    "homepage": "https://github.com/saviorenato/version",
    "keywords": ["laravel", "version"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Savio Pereira",
            "email": "savio.pereira@soluti.com.br"
        }
    ],
    "support": {
        "issues": "https://github.com/saviorenato/version/issues"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "config": {
        "platform": {
            "php": "8.1.0"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": ">=8.1"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    }
}
